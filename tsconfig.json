{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": false,
        "allowJs": false,
        "checkJs": true,
        "declaration": true,
        "declarationDir": "ts/d.ts",
        "noEmitHelpers": true,
        "alwaysStrict": true,
        "outDir": "ts/build"
    },
    "exclude": [
        "node_modules",
        "**/*.js",
        "ts/d.ts"
    ],
    "typeAcquisition": {
        "include": []
    }
}